
@{
    ViewData["Title"] = "ShowInfo";
}

<h1>ShowInfo</h1>
<p>
    Your Name : @ViewBag.Username
</p>
<p>
    Gender : @ViewBag.Gender
</p>
<p>
    City : @ViewBag.City
</p>
<p>
    Hobbies :
    @foreach (string item in ViewBag.Hobbies)
    {
       <span>@item</span> <br/>
    }
</p>
<p>
    Your Comment : @ViewBag.Comment
</p>